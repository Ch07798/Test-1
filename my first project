#include <stdio.h>

int main() {
    char name[21];
    int age;


    char myName[] = "chabota";
    int myAge = 10;
    char myHobby[] = "vollyball";


    printf("My name is %s.\n", myName);
    printf("I am %d years old.\n", myAge);
    printf("My favorite hobby is %s.\n", myHobby);


    printf("Enter your name: ");
    scanf("%99s", name);

    printf("Enter your age: ");
    scanf("%d", &age);


    printf("Hello, %s! You are %d years old.\n", name, age);

    return 0;
}
